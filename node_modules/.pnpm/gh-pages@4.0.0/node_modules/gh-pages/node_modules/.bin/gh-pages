#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/manel/program-sport/node_modules/.pnpm/gh-pages@4.0.0/node_modules/gh-pages/bin/node_modules:/Users/manel/program-sport/node_modules/.pnpm/gh-pages@4.0.0/node_modules/gh-pages/node_modules:/Users/manel/program-sport/node_modules/.pnpm/gh-pages@4.0.0/node_modules:/Users/manel/program-sport/node_modules/.pnpm/node_modules:/Users/manel/program-sport/node_modules:/Users/manel/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/manel/program-sport/node_modules/.pnpm/gh-pages@4.0.0/node_modules/gh-pages/bin/node_modules:/Users/manel/program-sport/node_modules/.pnpm/gh-pages@4.0.0/node_modules/gh-pages/node_modules:/Users/manel/program-sport/node_modules/.pnpm/gh-pages@4.0.0/node_modules:/Users/manel/program-sport/node_modules/.pnpm/node_modules:/Users/manel/program-sport/node_modules:/Users/manel/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/gh-pages.js" "$@"
else
  exec node  "$basedir/../../bin/gh-pages.js" "$@"
fi
